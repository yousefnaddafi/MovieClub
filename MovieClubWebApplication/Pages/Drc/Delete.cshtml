@page "{id}"
@model MovieClubWebApplication.Pages.Drc.DeleteModel
@{
    ViewData["Title"] = "Delete";
}

<h1>حذف کارگردان</h1>
<h1>Delete Confirmation</h1>
<div class="alert alert-danger">
    <h5>Are you sure you want to delete this list? - @Model.directorDLT.FullName</h5>
    <form method="post">
        <button type="submit"  class="btn btn-danger">Yes</button>
        <a class="btn btn-primary" asp-page="./Index">No</a>
    </form>
</div>




