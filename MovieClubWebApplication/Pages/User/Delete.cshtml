@page
@model MovieClubWebApplication.Pages.User.DeleteModel
@{
    ViewData["Title"] = "Delete User";
}
  
<h1>Delete Confirmation</h1>

<div class="alert alert-danger">
    <h5>Are you sure you wat to delete User - @Model.tempUser.Email</h5>
    <form method="post">
        <button type="submit" class="btn btn-danger">Yes</button>
        <a class="btn btn-primary" asp-page="/">No</a>
    </form>
</div>
